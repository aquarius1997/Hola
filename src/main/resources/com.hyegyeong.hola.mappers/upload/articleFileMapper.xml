<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hyegyeong.hola.mappers.upload.articleFileMapper">
    <insert id="addFile">
        INSERT INTO DIARY_ATTACHES (
                    DIARY_ID
                    ,MEMBER_ID
                    ,ORIG_NAME
                    ,FILE_NAME
                    ,FILE_EXT
                    )
             VALUES )
                    #{diaryId}
                    ,#{memberId}
                    ,#{orig_name}
                    ,#{file_name}
                    ,#{file_ext}
                    )
    </insert>
</mapper>